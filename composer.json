{
    "name": "kjjd84/lucid",
    "description": "Declare database schemas & factory definitions inside Laravel models.",
    "keywords": [
        "lucid",
        "database",
        "schemas",
        "factory",
        "definitions",
        "laravel",
        "models"
    ],
    "license": "MIT",
    "support": {
        "source": "https://github.com/kjjd84/lucid"
    },
    "authors": [
        {
            "name": "Kevin Dion",
            "email": "kejedi@icloud.com"
        }
    ],
    "require": {
        "doctrine/dbal": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Kjjd84\\Lucid\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kjjd84\\Lucid\\LucidServiceProvider"
            ]
        }
    }
}